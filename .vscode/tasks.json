                "build",
                "${workspaceFolder}/GloboTicket/GloboTicket.sln",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
